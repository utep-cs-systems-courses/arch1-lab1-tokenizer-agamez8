#list object files
CFLAGS=-g -O3 # -g for debug, -O3 for optimization

# file dependencies
simpleUI.o: tokenizer.h
tokenizer.o: tokenizer.h
history.o: history.h

all: simpleUI

tokenizer: simpleUI.o tokenizer.o history.o
	cc -o $@ $^ $(CFLAGS)

# delete files generated by compiler
clean:
	rm -f *.o *~ *# tokenizer

run: simpleUI
	./simpleUI
